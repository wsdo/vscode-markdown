{
	"name": "markdown",
	"displayName": "markdown",
	"description": "Markdown Paste Image To Qiniu OSS store",
	"version": "1.1.8",
  "publisher": "starkwang",
	"engines": {
		"vscode": "^1.56.0"
	},
  "author": {
    "name": "starkwang",
    "email": "wsd312@163.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wsdo/vscode-markdown-img-qiniu.git"
  },
  "homepage": "https://github.com/wsdo/vscode-markdown-img-qiniu.git/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/wsdo/vscode-markdown-img-qiniu.git/issues"
  },
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:markdown.qiniu"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "markdown.qiniu",
				"title": "Hello World"
			}
		],
		"keybindings": [
			{
				"command": "markdown.qiniu",
				"key": "ctrl+alt+v",
				"mac": "cmd+alt+v",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.56.0",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"child_process": "^1.0.2",
		"qiniu": "^7.3.3"
	}
}
